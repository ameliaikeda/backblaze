{
    "name": "amelia/backblaze",
    "type": "library",
    "description": "Backblaze laravel storage adapter",
    "keywords": ["b2", "backblaze", "flysystem", "storage", "laravel"],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Amelia Ikeda",
            "email": "amelia@dorks.io"
        }
    ],
    "require": {
        "php": ">=5.6",
        "mhetreramesh/flysystem-backblaze": "^1.0",
        "illuminate/support": "5.3.* | 5.4.* | 5.5.* | 5.6.*",
        "illuminate/cache": "5.3.* | 5.4.* | 5.5.* | 5.6.*"
    },
    "autoload": {
        "psr-4": {
            "Amelia\\Backblaze\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Amelia\\Backblaze\\BackblazeServiceProvider"
            ]
        }
    }
}
